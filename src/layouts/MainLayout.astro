---
import BaseHead from "../components/BaseHead.astro";
import Navbar from '../components/Navbar/index.astro';
import Footer from "../components/Footer/index.astro";

interface Props {
  title: string;
}
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<BaseHead title={title} />
	<body>
		<Navbar />
		<main>
			<slot />
		</main>
		<Footer />
	</body>
</html>
